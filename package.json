{
  "name": "hello-world-javascript-action",
  "version": "0.0.1",
  "private": false,
  "description": "Posts a nice qoute for pushing code.",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run pack"
  },
  "keywords": [
    "actions",
    "hello",
    "github"
  ],
  "author": {
    "name":"Alpha Olomi",
    "url": "https://alphaolomi.me"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/parser": "^4.3.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^7.10.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphaolomi/hello-world-javascript-action.git"
  },
  "bugs": {
